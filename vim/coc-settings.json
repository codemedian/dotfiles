{
    "languageserver": {
        "cairo": {
            "command": "~/Projects/sidehustle/cairo/target/release/cairo-language-server",
            "rootPatterns": [".git/", "hardhat.config.ts"],
            "trace.server": "verbose",
            "filetypes": ["cairo"]
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"]
        }
    }
}
